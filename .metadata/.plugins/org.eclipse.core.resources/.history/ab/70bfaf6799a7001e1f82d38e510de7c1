class Student{
	
	//HAS-A (variavles)
	String name;
	int id;
	float height;
	
}

class Car{
	
	//HAS-A part (variable)
	String brandName;
	int noOfWheel;
	String color;
}

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		short s = 1;// memmory for this short data -> 16 bits
		byte b = 1;// memmory for this byte data -> 8 bits
		
	}

}
